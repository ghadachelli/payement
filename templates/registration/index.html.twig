{# templates/registration/index.html.twig #}

{% extends 'base.html.twig' %}

{% block title %}Inscription{% endblock %}

{% block body %}
    <h1>Inscription</h1>

    {{ form_start(form) }}
        {{ form_row(form.nom_prenom) }}
        {{ form_row(form.email) }}
        {{ form_row(form.numtel) }}
        {{ form_row(form.mdp) }}
        {{ form_row(form.sexe) }}
        {{ form_row(form.dateNaisssance) }}

        <button type="submit" class="btn btn-primary">S'inscrire</button>
    {{ form_end(form) }}
{% endblock %}
